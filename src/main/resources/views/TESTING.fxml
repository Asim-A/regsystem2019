                </Menu>
                <Menu text="Help">
                  <items>
                    <MenuItem text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <Pane prefHeight="530.0" prefWidth="783.0">
               <children>
                  <JFXTextField fx:id="inputField" layoutX="324.0" layoutY="90.0" />
                  <JFXButton fx:id="loadBtn" layoutX="371.0" layoutY="180.0" onAction="#changeLabel" style="-fx-background-color: Grey;" text="Load" textFill="WHITE" />
